<%= form_with url: "/garages", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :city %>
  <%= form.text_field :city %>

  <%= form.label :open_to_public %>
  <%= form.check_box :open_to_public %>

  <%= form.label :max_capacity, "Max Capacity" %>
  <%= form.text_field :max_capacity %>

  <%= form.submit "Create Garage" %>
<% end %>